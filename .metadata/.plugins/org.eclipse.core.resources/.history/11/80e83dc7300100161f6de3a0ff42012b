import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
//		// TODO Auto-generated method stub
//		Scanner sc = new Scanner(System.in);
//		
//		int num = sc.nextInt();
//		
//		int minDigit = num / 25 + ((num % 25 == 0) ? 1 : 2);
