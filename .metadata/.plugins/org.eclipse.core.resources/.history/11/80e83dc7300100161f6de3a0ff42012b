		Scanner scanIn = new Scanner(System.in);
		int randRange = 5;
		
		System.out.println("Guess: ");
		int guess = scanIn.nextInt();
		
		if (guess != randRange) {
			if (guess > randRange) {
				System.out.println("Too large");
			} else if (guess < randRange) {
				System.out.println("Too small");
			} 
			
			System.out.println("Guess: ");
			guess = scanIn.nextInt();
			

			if (guess != randRange) {
				if (guess > randRange) {
					System.out.println("Too large");
				} else if (guess < randRange) {
					System.out.println("Too small");
				} 
				
				System.out.println("Guess: ");
				guess = scanIn.nextInt();
				
				
			} 
		} 
		
		if (guess > randRange) {
			System.out.println("Too large");
		} else if (guess < randRange) {
			System.out.println("Too small");
		} else {
			System.out.println("Congratualtions");
		}
		
		
	}

}
