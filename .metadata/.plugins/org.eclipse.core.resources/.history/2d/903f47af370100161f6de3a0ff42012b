import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		
		boolean isEven = num % 25 == 0;
		
		int minDigit = num / 25 + 1;
		
		if (!isEven) minDigit++;

		System.out.println(minDigit);
	}

}
